    <script type="text/javascript">
        $(document).ready(function () {
            $('#fullpage').fullpage({
                verticalCentered: false,
                navigation: true,
                navigationPosition: 'left',
                //to avoid problems with css3 transforms and fixed elements in Chrome, as detailed here: https://github.com/alvarotrigo/fullPage.js/issues/208
                anchors: ['home', 'platforms', 'progress', 'contactus'],
                menu: '#menu',
                css3: false,
                scrollOverflow: true,
                afterLoad: function (anchorLink, index) {
                    var loadedSection = $(this);
                    //using anchorLink
                    if (anchorLink == 'home') {
                        $('#homeanima').show();
                        $('#homeanima').animo({animation: ['fadeInUp'], duration: 0.7});
                    }
                    if (anchorLink == 'platforms') {
                        $('#platformsanima').show();
                        $('#platformsanima').animo({animation: ['fadeIn'], duration: 0.9});
                    }
                    if (anchorLink == 'progress') {
                        $('#Steps').show();
                        $('#Steps').animo({animation: ['fadeIn'], duration: 0.9});
                    }
                    if (anchorLink == 'contactus') {
                        $('#contactusanima').show();
                        $('#contactusanima').animo({animation: ['fadeIn'], duration: 0.9});
                    }
                },
                onLeave: function (index, nextIndex, direction) {
                    var leavingSection = $(this);

                    if (index == 1) {
                        $('#homeanima').hide();
                    }

                    if (index == 2) {
                        $('#platformsanima').hide();
                    }
                    if (index == 3) {
                        $('#Steps').hide();
                    }

                    if (index == 4) {
                        $('#contactusanima').hide();
                    }
                }
            });
        });
        $(window).load(function () {
            //  $('#homeanima').addClass('animated slideInUp');
            //  $('#platformsanima').addClass('animated slideInRight');
            //  $('#signupformanima').addClass('animated slideInLeft');
            // $('#homeanima').animo({animation: ['bounceInRight'], duration: 3});
            // $('#platformsanima').animo({animation: ['bounceInLeft'], duration: 3});
            $('#homeanima').hide();
            $('#platformsanima').hide();
            $('#Steps').hide();
            $('#contactusanima').hide();
        });
    </script>
